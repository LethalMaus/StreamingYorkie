    <string name="guide_menu_title">Menu Icons</string>
    <string name="guide_menu_description">Several menus are available to divide StreamingYorkie into each feature it offers:</string>
    <string name="guide_menu_followers">Shows all <b>Followers</b> from Twitch including who has unfollowed</string>
    <string name="guide_menu_following">Shows all <b>Following</b> from Twitch including who you have unfollowed</string>
    <string name="guide_menu_f4f">Overview &amp; simplification users who <b>F4F (Follow for Follow)</b></string>
    <string name="guide_menu_vods">Overview for VODs (Videos On Demand) &amp; exporting VODs</string>
    <string name="guide_menu_multi">Watch multiple streams at once on one screen</string>
    <string name="guide_menu_lurk">Lurk streamers and add to their view count</string>
    <string name="guide_menu_userinfo">Quick overview about your Twitch account</string>
    <string name="guide_menu_info">Shows different platforms for further information such as a <b>Twitch</b> &amp; <b>Contact</b></string>
    <string name="guide_menu_github_readme">Github link to ReadMe guide</string>
    <string name="guide_menu_help">Offline help guide</string>
    <string name="guide_menu_updates">Github link to Updates</string>
    <string name="guide_menu_sourcecode">Github link to the source code</string>
    <string name="guide_menu_contact">Github link to Contact</string>
    <string name="guide_menu_discord">Link to Discord</string>
    <string name="guide_menu_twitch">Link to developers stream</string>
    <string name="guide_menu_patreon">Membership</string>
    <string name="guide_menu_github">Projects</string>
    <string name="guide_menu_settings">The settings menu is split up into further features below</string>
    <string name="guide_menu_autofollow">Settings for activating &amp; configuring the AutoFollow Worker</string>
    <string name="guide_menu_autovodexport">Settings for activating &amp; configuring the AutoVODExport Worker</string>
    <string name="guide_menu_autolurk">Settings for activating &amp; configuring the AutoLurk Worker</string>
    <string name="guide_menu_authorization">Logs out of StreamingYorkie &amp; Twitch as well as deleting any data relating to your Twitch account</string>
    <string name="guide_categories_title">Category Icons</string>
    <string name="guide_categories_description">Each Menu is split into 3&#8211;4 of the following categories:</string>
    <string name="guide_categories_new">All new Followers/Following</string>
    <string name="guide_categories_follow">All current Followers/Following (including New)</string>
    <string name="guide_categories_unfollow">All Followers who Unfollowed you or Following you Unfollowed</string>
    <string name="guide_categories_excluded">Users/VODs that are excluded from AutoFollow/AutoExport as well as being excluded from the other menus</string>
    <string name="guide_categories_notfollowing_followers">Users who Follow you, but you dont Follow them back</string>
    <string name="guide_categories_follow4follow">Followers who also Follow you</string>
    <string name="guide_categories_following_nonfollowers">Users you Follow, who dont Follow you back</string>
    <string name="guide_categories_vods">VODs (Videos On Demand) that are available on your Twitch account</string>
    <string name="guide_categories_export">Exported VODs done by StreamingYorkie will be found here</string>
    <string name="guide_actions_title">Action Icons</string>
    <string name="guide_actions_description">Each category contains up to 3 of the following actions:</string>
    <string name="guide_actions_follow">Follows the chosen User</string>
    <string name="guide_actions_unfollow">Unfollows the chosen User</string>
    <string name="guide_actions_notifications">Activates notifications received from the User</string>
    <string name="guide_actions_notifications_deactivate">Deactivates notifications received from the User</string>
    <string name="guide_actions_exclude">Excludes Users/VODs from other categories &amp; from AutoFollow</string>
    <string name="guide_actions_include">Includes Users/VODs from other categories &amp; from AutoFollow</string>
    <string name="guide_actions_delete">Deletes User/VOD from the Device (not Twitch)</string>
    <string name="guide_actions_export">Exports a VOD from Twitch to Youtube</string>
    <string name="guide_actions_multi">Start watching multiple streams at once on one screen</string>
    <string name="guide_actions_lurk">Initiate lurking given streamer</string>
    <string name="guide_actions_unlurk">Stop lurking chosen streamer</string>
    <string name="guide_actions_message">Opens a dialog to send a message to the chosen channels chat</string>
    <string name="guide_actions_send">Sends the message to channel chat</string>
    <string name="guide_actions_cancel">Cancels the current action being taken</string>
    <string name="guide_actions_refresh">Refreshes the current view (if internet is available)</string>
    <string name="guide_actions_authorization">Logs out of StreamingYorkie &amp; Twitch as well as deleting all device data</string>
    <string name="guide_actions_save">Saves the current changes (if any have been made)</string>
